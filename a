#!/bin/awk -f

BEGIN {
  #nastaveni separatoru zaznamu na znak <
  #nastaveni separatoru pole na znak > 
  RS="<"
  FS=">"
}
#tedy adresa bude vzdycky v $1

#odstrani nadbytecni mezery a novy radky
gsub(/^a[[:space:]]+href[[:space:]]*=[[:space:]]*/, "a href=", $1)

{
  if ($1 ~ /a href=['"][^"']*['"]/) {
    sub(/a href=["']/, "", $1)
    sub(/["']/, "", $1)

    gsub("&quot;", "\"", $1)  
    gsub("&lt;", "<", $1)
    gsub("&gt;", ">", $2)
  
    print $1
  }
} 
